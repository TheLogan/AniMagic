{
  "name": "animagic",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Tim Logan",
    "url": "https://github.com/TheLogan/AniMagic"
  },
  "build": {
    "appId": "com.thelogan.animagic",
    "productName": "AniMagic",
    "copyright": "Copyright Â© 2019 ${Tim Logan}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@rescripts/cli": "0.0.13",
    "@rescripts/rescript-env": "0.0.11",
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^25.1.2",
    "@types/johnny-five": "^1.3.0",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "concurrently": "^5.1.0",
    "electron-is-dev": "^1.1.0",
    "gojs": "^2.1.8",
    "gojs-react": "^1.0.5",
    "johnny-five": "^1.4.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0",
    "wait-on": "^4.0.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "electron": "electron .",
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder build -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^3.4.7",
    "electron": "^9.4.0",
    "electron-builder": "^22.3.2"
  }
}
